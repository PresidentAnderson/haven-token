# HAVEN Token Environment Variables
# Copy this file to .env.local and fill in your values

# Contract Addresses
NEXT_PUBLIC_HAVEN_CONTRACT_ADDRESS=0x... # HAVEN token contract address on Base
NEXT_PUBLIC_HAVEN_CONTRACT_ADDRESS_TESTNET=0x... # HAVEN token contract address on Base Sepolia

# RPC Configuration
NEXT_PUBLIC_ALCHEMY_API_KEY=your_alchemy_api_key_here
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org
NEXT_PUBLIC_BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# WalletConnect Project ID (get from https://cloud.walletconnect.com)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Network Configuration
NEXT_PUBLIC_ENABLE_TESTNETS=true
NEXT_PUBLIC_DEFAULT_CHAIN=base # or base-sepolia

# Application Settings
NEXT_PUBLIC_SITE_URL=https://haven-token.com
NEXT_PUBLIC_BASESCAN_URL=https://basescan.org
NEXT_PUBLIC_BASESCAN_TESTNET_URL=https://sepolia.basescan.org

# Analytics (optional)
NEXT_PUBLIC_GA_MEASUREMENT_ID=
